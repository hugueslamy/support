!!! 5
/[if lt IE 7]
  %html.no-js.lt-ie9.lt-ie8.lt-ie7
/[if IE 7]
  %html.no-js.lt-ie9.lt-ie8
/[if IE 8]
  %html.no-js.lt-ie9
%html.no-js{:lang=>"en"}
  %head
    %meta{ :charset => "utf-8" }
    %meta{ 'http-equiv' => "X-UA-Compatible", :content => "IE=edge" }
    %title OnApp support test
    %meta{ :name => "description", :content => "This is my work on the support tool creation test" }
    %meta{ :name => "viewport", :content => "width=device-width, initial-scale=1" }
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'modernizr-2.6.2.min.js'
    = csrf_meta_tags

  %body
    .container
      = render 'layouts/nav'
      - if notice
        %p.notice= notice
      - if alert
        %p.alert= alert 

      = yield
      
      = javascript_include_tag 'application', 'data-turbolinks-track' => true
      = render 'layouts/analytics'
